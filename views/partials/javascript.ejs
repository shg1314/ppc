<!--제휴하기 팝업창 열기-->
<script language="javascript">
    function showPopup() { window.open("/partner_enquiry", "a", "width=600, height=600, resizable=no ,left=100, top=50"); }
</script>

<!--팝업창 동의하기 체크박스-->
<SCRIPT LANGUAGE="JavaScript">
    function chk() {
        var num = 0;	
        var req = document.partner_enquiry.req.checked;;
        
        if(req==true){
            num =1 ;
        }

        if(num==1){
            document.partner_enquiry.submit();
            alert("담당자가 문의사항을 접수후 빠른시일 안에 연락드리겠습니다. 감사합니다.")
        }else{
            alert("개인정보 수집 약관에 동의해주세요");
            return false;
        }
    }
</SCRIPT>

